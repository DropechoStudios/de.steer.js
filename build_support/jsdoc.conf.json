{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./src/","./README.md"],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks"     : true,
        "monospaceLinks"  : true,
        "systemName"      : "desteer.js",
        "footer"          : "",
        "copyright"       : "",
        "navType"         : "vertical",
        "theme"           : "cyborg",
        "linenums"        : false,
        "collapseSymbols" : false,
        "inverseNav"      : false
    },
    "opts": {
        "template": "build_support/jsdoc/templates/docstrap",
        "destination": "./docs/",
        "recurse": true,
        "tutorials": ""       
    },
    "jsVersion": 180
}